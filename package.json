{
  "name": "teatime-components",
  "version": "0.10.9",
  "description": "React components + CSS Modules",
  "main": "index.js",
  "scripts": {
    "docs": ".script/docs",
    "dist": "webpack --config .config/dist.config.js",
    "fix": "eslint --fix {component,decorator,lib,test,view}/**/*.js *.js",
    "lint": "eslint {component,decorator,lib,test,view}/**/*.js *.js",
    "start": "npm run styleguide",
    "styleguide": "styleguidist --config .config/styleguide.config.js server",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sullenor/teatime-components.git"
  },
  "author": "Alexey Litvinov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sullenor/teatime-components/issues"
  },
  "homepage": "https://github.com/sullenor/teatime-components#readme",
  "eslintConfig": {
    "extends": "@sullenor/eslint-config-react",
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "no-case-declarations": 0
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/test/enzyme.setup"
    ]
  },
  "peerDependencies": {
    "react": "^15.5.x || 16.x",
    "react-dom": "^15.5.x || 16.x"
  },
  "devDependencies": {
    "@sullenor/eslint-config-react": "^1.0.10",
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-2": "^6.22.0",
    "css-loader": "^0.28.4",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^3",
    "eslint-plugin-react": "^6.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "postcss-cli": "^4.1.0",
    "postcss-loader": "^1.3.2",
    "postcss-url": "5",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-styleguidist": "^5.5.0",
    "react-test-renderer": "^16.0.0",
    "style-loader": "^0.16.1",
    "webpack": "^2.6.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "fuzzysearch": "^1.0.3",
    "lodash.chunk": "^4.2.0",
    "prop-types": "^15.6.0"
  }
}
